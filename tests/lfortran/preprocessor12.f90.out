program preprocessor12
! Test that a typical ASSERT macro works
implicit none
if(.not.(.true.)) call f90_assert("preprocessor12.f90",7)
if(.not.(5 > 3)) call f90_assert("preprocessor12.f90",8)
if(.not.(5+3*8 > 3)) call f90_assert("preprocessor12.f90",9)
if(.not.(fn(3, 5))) call f90_assert("preprocessor12.f90",11)
if(.not.(5 < 3)) call f90_assert("preprocessor12.f90",13)
contains
logical function fn(a, b)
integer, intent(in) :: a, b
fn = a < b
end function
subroutine f90_assert(file, line)
character(len=*), intent(in) :: file
integer,          intent(in) :: line
print *, "Assertion failed at " // file // ":", line
error stop
end subroutine f90_assert
end program
